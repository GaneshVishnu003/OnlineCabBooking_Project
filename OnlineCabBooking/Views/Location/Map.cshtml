
@{
    ViewBag.Title = "Map";
    Layout = null;
}
    <head>
        <script src="~/Scripts/jquery-3.4.1.min.js"></script>
        <title>

        </title>
        <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBGdl5m_0Rvsks8aBTKnbwsNdsctmuOn3Q&callback=initMap"
                type="text/javascript"></script>
        <style>
            #map {
                height: 100%;
                width: 100%;
            }
        </style>
    </head>
<body>
    <h2>Map</h2>
    <script>
        function initMap() {
            var options = {
                zoom: 12,
                center: { lat: 13.0827, lng: 80.2707 }
            }
            var map = new google.maps.Map(document.getElementById('map'), options);
            var marker = new google.maps.Marker({
                position: { lat: 13.0827, lng: 80.2707 },
                map: map
            });
        }
    </script>
    <script>
        var maps = new google.maps.Map(document.getElementById('map'), {
            zoom: 4
        });
    </script>
    <div id="map"> 
  
    </div>

</body>

